{
  "name": "@codeplant-de/prisma-transactions",
  "version": "1.1.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/codeplant-de/nodejs-server-utils",
    "directory": "packages/prisma-transactions",
    "type": "git"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "lint": "yarn cp-lint",
    "format": "yarn cp-format",
    "pretest": "rm ./prisma/test.db && yarn prisma db push",
    "test": "yarn jest",
    "build": "rimraf lib && yarn build:cjs && yarn build:types",
    "build:cjs": "yarn cp-transpile -o lib cjs",
    "build:types": "yarn cp-transpile -o lib types"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@prisma/client": "^4.15.0",
    "jest": "^27.0.0",
    "prisma": "^4.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
