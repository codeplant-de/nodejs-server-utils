  "dependencies": {
    "apollo-server-types": "^3.7.1",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "apollo-server-errors": "^3.3.1",
    "apollo-server-plugin-base": "^3.7.1",
    "jest-extended": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
